package LabbookL_1;

public class TrafficLight {

	public static void main(String[] args) {
		int number=0;
		switch(number) {
		case 0:System.out.println("Stop");
				break;
		case 1:System.out.println("ready");
				break;
		case 2: System.out.println("go");
				break;
		default:break;		
		}
		

	}

}
