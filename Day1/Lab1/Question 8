package LabbookL_1;

public class powerOfTwo {
	public static boolean checkNumber(int n)
	{
		if (n==0)
			return false;
		while(n != 1)
		{
			if (n%2 !=0 )
			{
				return false;
			}
			n=n/2;
		}
		return true;
	}
	public static void main(String[] args) {
		boolean flag=checkNumber(8);
		if (flag)
			System.out.println("Power of 2");
		else
			System.out.println("not power of 2");
		
	}

}
